{
  "compilerOptions": {
    "module": "commonjs",
    "removeComments": true,
    "target": "es6",
    "jsx": "react",
    "noImplicitAny": true,
    "experimentalDecorators": true,
    "preserveConstEnums": true,
    "outDir": "built",
    "rootDir": "src",
    "sourceMap": true,
    "allowJs": true,
    "moduleResolution": "node"
  }
}
